#include "WPILib.h"
#define PORT_DRIVE_VIC_1 5
#define PORT_DRIVE_VIC_2 10
#define PORT_DRIVE_VIC_3 2
#define PORT_DRIVE_VIC_4 1
#define PORT_JS_SPEED 1
#define PORT_JS_TURN 2
#define Port_JS_OPERATOR 3


class MyRobot : public IterativeRobot {
Victor leftVic1;
Victor leftVic2;
Victor rightVic1;
Victor leftVic2;
Joystick speedStickX;
Joystick speedStickY
Joystick turnStickX;
Joystick turnStickY
Joystick operatorStickX;
Joystick operatorStickY

public:
MyRobot () :
  leftVic1(PORT_DRIVE_VIC_1),
  leftVic2(PORT_DRIVE_VIC_2),
  rightVic1(PORT_DRIVE_VIC_3),
  rightVic2(PORT_DRIVE_VIC_4),
  speedStickX(PORT_JS_SPEED),
  speedStickY(Port_JS_SPEED),
  turnStickX(PORT_JS_TURN),
  turnStickY(PORT_JS_TURN),
  operatorStickX(PORT_JS_OPERATOR)
  operatorStickY(PORT_JS_OPERATOR)
{


}
  
void HumanController(){
  
  if (operatorStickX operatorStickX*)
  leftVic1.Set(0.5);
  leftVic2.Set(0.5);
  rightVic1.Set(-0.5);
  rightVic2.Set(-0.5);
  Wait(2.0);
  leftVic1.Set(0);
  leftVic2.Set(0);
  rightVic1.Set(0);
  rightVic2.Set(0);
  if (operatorStickY operatorStickY*)
  leftVic1.Set(-0.5);
  leftVic2.Set(-0.5);
  rightVic1.Set(0.5);
  rightVic2.Set(0.5);
  Wait(2.0);
  leftVic1.Set(0);
  leftVic2.Set(0);
  rightVic1.Set(0);
  rightVic2.Set(0);
}
};
  
  START_ROBOT_CLASS(MyRobot);
